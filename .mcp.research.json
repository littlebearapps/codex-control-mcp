{
  "mcpServers": {
    "codex-control": {
      "command": "node",
      "args": [
        "/Users/nathanschram/claude-code-tools/lba/apps/mcp-servers/codex-control/dist/index.js"
      ],
      "env": {
        "CODEX_MAX_CONCURRENCY": "2"
      }
    },
    "brave-search": {
      "command": "bash",
      "args": [
        "-c",
        "source ~/bin/kc.sh && source ~/.config/kc/profiles.sh && load_mcp_secrets codex-control main && npx -y @modelcontextprotocol/server-brave-search"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ]
    }
  }
}
