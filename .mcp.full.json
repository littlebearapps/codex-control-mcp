{
  "mcpServers": {
    "mcp-delegator": {
      "command": "mcp-delegator",
      "env": {
        "CODEX_MAX_CONCURRENCY": "2",
        "CODEX_LOG_FILE": "${PWD}/.codex-errors.log",
        "CODEX_LOG_LEVEL": "debug"
      }
    },
    "brave-search": {
      "command": "bash",
      "args": [
        "-c",
        "source ~/bin/kc.sh && source ~/.config/kc/profiles.sh && load_mcp_secrets mcp-delegator main && npx -y @modelcontextprotocol/server-brave-search"
      ]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp-server"]
    },
    "mult-fetch": {
      "command": "npx",
      "args": ["-y", "@punkpeye/mcp-server-fetch"]
    }
  }
}
